{
  "a": [".text-color-primary", ".cursor-pointer"],
  ".box": [".pad-s", ".shadow-s"],
  ".button": [
    ".bdr-xs",
    ".bdr-color-primary",
    ".bg-primary",
    ".inline-block",
    ".pad-horz-s",
    ".text-color-white",
    ".text-height-l",
    ".text-size-m",
    ".bdr-radius-l",
    ".text-nowrap",
    ".text-height-xl",
    ".text-align-center"
  ],
  ".button:focus": [".bg-secondary", ".trans-all-fast", ".text-color-white"],
  ".button:hover": [".bg-secondary", ".trans-all-fast", ".text-color-white"],
  ".button:active": [".opac-70", ".trans-all-fast", ".shadow-s"],
  ".button.disabled": [".bg-grey-light", ".opac-50", ".bdr-color-grey-light"],
  ".button.disabled:hover": [".bg-grey-light", ".opac-50"],
  ".button.disabled:active": [".bg-grey-light", ".opac-50"],
  ".button.large": [".text-size-l", ".text-height-xxl"],
  ".button.medium": [".text-height-l"],
  ".button.small": [".text-height-m"],
  ".button.danger": [".text-color-white", ".bg-danger", ".bdr-color-danger"],
  ".button.danger:hover": [".bg-white", ".text-color-danger"],
  ".button.danger:active": [".opac-50"],
  ".button.secondary": [".text-color-primary", ".bg-white"],
  ".button.secondary:hover": [
    ".bg-secondary",
    ".trans-all-fast",
    ".text-color-white"
  ],
  ".button.secondary:active": [".text-color-primary", ".bg-white", ".shadow-s"],
  ".button.secondary.disabled": [
    ".text-color-grey",
    ".bdr-xs",
    ".bdr-color-white",
    ".bg-white"
  ],
  ".button.secondary.disabled:hover": [
    ".text-color-grey",
    ".bdr-xs",
    ".bdr-color-white",
    ".bg-white"
  ],
  ".button.secondary.disabled:active": [
    ".text-color-grey",
    ".bdr-xs",
    ".bdr-color-white",
    ".bg-white"
  ],
  ".column": [".flex-col"],
  ".columns": [".flex", ".flex-dir-row"],
  ".list": [""],
  ".print": [""],
  "blockquote": [""],
  "code": [".text-size-s"],
  "em": [".text-font-primary-italic"],
  "footer": [""],
  "form": [""],
  "h1": [".text-size-xxl", ".text-font-primary", ".text-font-primary-bold"],
  "h2": [".text-size-xl", ".text-font-primary", ".text-font-primary-bold"],
  "h3": [".text-size-l", ".text-font-primary", ".text-font-primary-bold"],
  "h4": [".text-size-m", ".text-font-primary", ".text-font-primary-bold"],
  "h5": [".text-size-s", ".text-font-primary", ".text-font-primary-bold"],
  "hr": [""],
  "nav": [""],
  "p": [".text-size-m", ".text-height-m", ".text-color-grey"],
  "section": [".pad-vert-s"],
  "section": [""],
  "small": [".text-size-s"],
  "strong": [".text-font-primary-bold"],
  "strong > em, em > strong": [".text-font-primary-bold-italic"],
  "table": [""],
  "ul": [".text-size-m"]
}