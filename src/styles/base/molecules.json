{
  "a": [
    ".text-color-primary",
    ".cursor-pointer",
    ".text-size-m",
    ".text-underline-none",
    ".inline-block",
    ".bdr-none"
  ],
  ".box": [".pad-s", ".shadow-s"],
  ".button": [
    ".bdr-xs",
    ".bdr-color-primary",
    ".bg-primary",
    ".inline-block",
    ".pad-horz-s",
    ".text-color-white",
    ".text-height-l",
    ".text-size-m",
    ".bdr-radius-l",
    ".text-nowrap",
    ".text-height-xl",
    ".text-align-center",
    ".text-underline-none"
  ],
  ".button:focus": [".bg-secondary", ".anim-speed-fast", ".text-color-white"],
  ".button:hover": [".bg-secondary", ".anim-speed-fast", ".text-color-white"],
  ".button:active": [".opac-70", ".anim-speed-fast", ".shadow-s"],
  ".button.disabled": [".bg-grey-light", ".opac-50", ".bdr-color-grey-light"],
  ".button.disabled:hover": [".bg-grey-light", ".opac-50"],
  ".button.disabled:active": [".bg-grey-light", ".opac-50"],
  ".button.large": [".text-size-l", ".text-height-xxl"],
  ".button.medium": [".text-height-l"],
  ".button.small": [".text-height-m"],
  ".button.danger": [".text-color-white", ".bg-danger", ".bdr-color-danger"],
  ".button.danger:hover": [".bg-white", ".text-color-danger"],
  ".button.danger:active": [".opac-50"],
  ".button.secondary": [".text-color-primary", ".bg-white"],
  ".button.secondary:hover": [
    ".bg-secondary",
    ".anim-speed-fast",
    ".text-color-white"
  ],
  ".button.secondary:active": [".text-color-primary", ".bg-white", ".shadow-s"],
  ".button.secondary.disabled": [
    ".text-color-grey",
    ".bdr-xs",
    ".bdr-color-white",
    ".bg-white"
  ],
  ".button.secondary.disabled:hover": [
    ".text-color-grey",
    ".bdr-xs",
    ".bdr-color-white",
    ".bg-white"
  ],
  ".button.secondary.disabled:active": [
    ".text-color-grey",
    ".bdr-xs",
    ".bdr-color-white",
    ".bg-white"
  ],
  "code": [".text-font-monospace", ".text-size-s"],
  ".column": [".flex-col"],
  ".column.half": [".flex-col-half"],
  ".column.full": [".flex-col-full"],
  ".column.third": [".flex-col-third"],
  ".column.fourth": [".flex-col-fourth"],
  ".column.fifth": [".flex-col-fifth"],
  ".column.sixth": [".flex-col-sixth"],
  ".columns": [".flex", ".flex-dir-row", ".flex-wrap"],
  ".icon": [
    ".block",
    ".bg-cover",
    ".bg-center",
    ".mrgn-auto",
    ".height-l",
    ".width-l"
  ],
  ".icon.black": [".filter-color-black"],
  ".icon.white": [".filter-color-white"],
  ".icon.l": [".height-l", ".width-l"],
  ".icon.m": [".height-m", ".width-m"],
  ".icon.s": [".height-s", ".width-s"],
  ".icon.xl": [".height-xl", ".width-xl"],
  ".icon.xs": [".height-xs", ".width-xs"],
  ".icon.xxl": [".height-xxl", ".width-xxl"],
  ".list": [""],
  ".print": [""],
  "blockquote": [""],
  "code": [".text-size-s"],
  "em": [".text-font-primary-italic"],
  "footer": [""],
  "form": [""],
  "h1": [".text-size-xxl", ".text-font-primary", ".text-space-s"],
  "h2": [
    ".text-size-xl",
    ".text-font-primary",
    ".text-space-s",
    ".bdr-bot-xs",
    ".bdr-bot-color-grey-light-tint",
    ".pad-bot-s"
  ],
  "h3": [
    ".text-size-l",
    ".text-font-primary-bold",
    ".text-space-xs",
    ".mrgn-top-m",
    ".mrgn-bot-s"
  ],
  "h3+p": [".mrgn-top-xs-neg"],
  "h4": [".text-size-m", ".text-font-primary", ".text-space-s"],
  "h5": [".text-size-s", ".text-font-primary", ".text-space-s"],
  "hr": [""],
  "nav": [""],
  "p": [
    ".text-font-primary",
    ".text-size-m",
    ".text-height-m",
    ".text-space-xs",
    ".mrgn-vert-s"
  ],
  "pre": [".text-preformat"],
  "section": [".pad-vert-s"],
  "section": [""],
  "small": [".text-size-s"],
  "strong": [".text-font-primary-bold"],
  "strong > em, em > strong": [".text-font-primary-bold-italic"],
  "table": [".text-size-m", ".bdr-collapse"],
  "table th": [
    ".bdr-bot-xs",
    ".bdr-color-grey-light-tint",
    ".text-font-bold",
    ".pad-horz-s",
    ".text-size-s",
    ".pad-vert-xs",
    ".text-case-upper",
    ".text-space-s",
    ".text-align-left"
  ],
  "table td": [
    ".pad-vert-xs",
    ".pad-horz-s",
    ".bdr-bot-xs",
    ".bdr-color-grey-light-tint"
  ],
  "[data-tooltip]": [".pos-rel"],
  "[data-tooltip]:hover:after": [
    ".content-attr-tooltip",
    ".pad-xs",
    ".bg-grey-light-tint",
    ".text-underline-none",
    ".text-color-grey",
    ".mrgn-horz-xs",
    ".text-size-xs",
    ".bdr-none",
    ".pos-abs",
    ".pos-left-none",
    "top:calc(-100% - 24px)"
  ],
  "ul": [".text-size-m"]
}
