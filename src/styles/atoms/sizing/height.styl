$sizing-height =
    &-unit-2xs {
        height: 1rem;
    }

    &-unit-xs {
        height: 2rem;
    }

    &-unit-s {
        height: 3rem;
    }

    &-unit-base {
        height: 4rem;
    }

    &-unit-l {
        height: 5rem;
    }

    &-unit-xl {
        height: 6rem;
    }

    &-unit-2xl {
        height: 8rem;
    }

    &-unit-3xl {
        height: 10rem;
    }

    &-unit-4xl {
        height: 12rem;
    }

    &-unit-5xl {
        height: 16rem;
    }

    &-auto {
        height: auto;
    }

    &-px {
        height: 1px;
    }

    &-1\\/2 {
        height: 50%;
    }

    &-1\\/3 {
        height: 33.33333%;
    }

    &-2\\/3 {
        height: 66.66667%;
    }

    &-1\\/4 {
        height: 25%;
    }

    &-3\\/4 {
        height: 75%;
    }

    &-1\\/5 {
        height: 20%;
    }

    &-2\\/5 {
        height: 40%;
    }

    &-3\\/5 {
        height: 60%;
    }

    &-4\\/5 {
        height: 80%;
    }

    &-1\\/6 {
        height: 16.66667%;
    }

    &-5\\/6 {
        height: 83.33333%;
    }

    &-max-full {
        max-height: 100%;
    }

    &-full {
        height: 100%;
    }

    &-min-full {
        min-height: 100%;
    }

    &-screen {
        height: 100vh;
    }

    &-min-screen {
        min-height: 100vh;
    }

.height {
    {$sizing-height};
}

mq('height', $sizing-height);